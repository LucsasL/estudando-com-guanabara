algoritmo "Contador_10a0"
var
   contador_r: Inteiro
inicio
      contador_r <- 10
      Enquanto (contador_r >= 1) faca
               Escreval(contador_r)
               contador_r <- contador_r - 1
      Fimenquanto
      Escreval("-------------------")
      Escreval("terminei de contar.")
fimalgoritmo